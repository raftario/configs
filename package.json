{
	"name": "@raftario/configs",
	"version": "0.0.0",
	"type": "module",
	"files": [
		"src",
		"target",
		"tsconfig.json"
	],
	"exports": {
		"./eslint": "./target/eslint.config.js",
		"./prettier": "./target/prettier.config.js",
		"./tsconfig.json": "./tsconfig.json"
	},
	"scripts": {
		"prepare": "tsc --project tsconfig.configs.json",
		"eslint": "eslint --config target/eslint.config.js .",
		"prettier": "prettier --config target/prettier.config.js ."
	},
	"peerDependencies": {
		"eslint": "^9.0",
		"prettier": "^3.1",
		"typescript": ">=4.9.0"
	},
	"dependencies": {
		"@eslint/compat": "^1.1",
		"@eslint/js": "^9.11",
		"eslint-config-prettier": "^9.1",
		"eslint-plugin-jsdoc": "^50.2",
		"eslint-plugin-jsx-a11y": "^6.10",
		"eslint-plugin-react-hooks": "^4.6",
		"eslint-plugin-simple-import-sort": "^12.1",
		"eslint-plugin-unicorn": "^55.0",
		"typescript-eslint": "^8.6"
	},
	"devDependencies": {
		"@types/eslint": "*",
		"@types/eslint__js": "*",
		"@types/node": "*",
		"eslint": "^9.10",
		"prettier": "^3.3",
		"typescript": "~5.6"
	},
	"overrides": {
		"eslint-plugin-jsx-a11y": {
			"eslint": "$eslint"
		},
		"eslint-plugin-react-hooks": {
			"eslint": "$eslint"
		}
	}
}
